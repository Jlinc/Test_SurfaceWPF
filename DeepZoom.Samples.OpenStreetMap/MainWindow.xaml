<Window x:Class="DeepZoom.Samples.OpenStreetMap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dz="clr-namespace:DeepZoom.Controls;assembly=DeepZoom"
        xmlns:osm="clr-namespace:DeepZoom.Samples.OpenStreetMap"
        Title="OpenStreetMap Deep Zoom" Height="800" Width="1280">
    <Border Background="White">
        <dz:MultiScaleImage>
            <dz:MultiScaleImage.Source>
                <osm:OpenStreetMapTileSource />
            </dz:MultiScaleImage.Source>
        </dz:MultiScaleImage>
    </Border>
</Window>
